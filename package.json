    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "stripe": "^9.0",
    "swagger-ui-express": "^4.4.0",
    "unique-names-generator": "^4.7.1",
